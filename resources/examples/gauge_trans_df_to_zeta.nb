Cell[CellGroupData[{

Cell["Solve \[Delta]t from the scalar inflaton", "Section",
 CellChangeTimes->{{3.5989519795073957`*^9, 3.598952031405263*^9}, {
  3.5989553073588743`*^9, 3.598955338280938*^9}, {3.5989568017794943`*^9, 
  3.598956830513804*^9}}],

Cell[TextData[{
 "The gauge transformation which brings \[Delta]\[Phi]-gauge (FG) to \
\[Zeta]-gauge (ZG) is\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["t", "ZG"], "=", 
     RowBox[{
      SubscriptBox["t", "FG"], "+", 
      RowBox[{"\[Delta]t", "(", 
       SubscriptBox["t", "FG"], ")"}]}]}], ",", " ", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["x", "ZG"], "i"], "=", 
     RowBox[{
      SuperscriptBox[
       SubscriptBox["x", "FG"], "i"], "+", 
      SuperscriptBox[
       RowBox[{"\[Delta]x", "(", 
        SubscriptBox["x", "FG"], ")"}], "i"]}]}]}], TraditionalForm]]],
 "\n\nUnder this gauge transformation, \[Phi] transforms as\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Phi]", "ZG"], "(", 
     RowBox[{
      SubscriptBox["t", "ZG"], ",", " ", 
      SubscriptBox["x", "ZG"]}], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Phi]", "0"], "(", 
      SubscriptBox["t", "ZG"], ")"}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Phi]", "ZG"], "(", 
       RowBox[{
        SubscriptBox["t", "FG"], "+", "\[Delta]t"}], ")"}], "=", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[Phi]", "FG"], "(", 
        RowBox[{
         SubscriptBox["t", "FG"], ",", " ", 
         SubscriptBox["x", "FG"]}], ")"}], "=", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[Phi]", "0"], "(", 
         SubscriptBox["t", "FG"], ")"}], "+", 
        RowBox[{
         SubscriptBox["\[Delta]\[Phi]", "FG"], "(", 
         RowBox[{
          SubscriptBox["t", "FG"], ",", " ", 
          SubscriptBox["x", "FG"]}], ")"}]}]}]}]}]}], TraditionalForm]]],
 "\n\nWe shall write ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Delta]\[Phi]", "FG"], TraditionalForm]]],
 " as ",
 Cell[BoxData[
  FormBox["\[Delta]\[Phi]", TraditionalForm]]],
 " because the corresponding quantity does not exist in ZG.\n\nFor \
simplicity, we write ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"\[Delta]x", "(", 
     SubscriptBox["x", "FG"], ")"}], "i"], TraditionalForm]]],
 "=",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"\[Delta]x", "(", 
     SubscriptBox["x", "FG"], ")"}], "i"], TraditionalForm]]],
 ", i.e. the index is raised/lowered by ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Delta]", "ij"], TraditionalForm]]],
 ", not ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["g", "ij"], TraditionalForm]]],
 ".\n"
}], "Text",
 CellChangeTimes->{{3.598955340513565*^9, 3.598955536076158*^9}, {
  3.598955808237414*^9, 3.598955916549437*^9}, {3.5989560030291147`*^9, 
  3.598956132759633*^9}, {3.5989562893170652`*^9, 3.598956292647691*^9}, {
  3.598956425972077*^9, 3.598956475582078*^9}, {3.598957851984748*^9, 
  3.598957863302251*^9}, {3.5989609065195427`*^9, 3.598960933937683*^9}, {
  3.598960995621985*^9, 3.59896105338155*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[Delta]tEq", "=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"expandAtXZG", "[", "\[Phi]0", "]"}], "\[Equal]", 
    RowBox[{"\[Phi]0", "+", 
     RowBox[{"Eps", " ", "\[Delta]\[Phi]"}]}]}], ")"}]}]], "Input",
 CellChangeTimes->{{3.5989561669928617`*^9, 3.598956175961158*^9}, {
  3.598956261995013*^9, 3.5989563513352118`*^9}, {3.598956393130475*^9, 
  3.598956408211609*^9}, {3.598956487235787*^9, 3.598956515995388*^9}, {
  3.598956703787423*^9, 3.598956717327711*^9}, {3.5989590634420557`*^9, 
  3.598959067634753*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"\[Phi]0", "+", 
   RowBox[{"Eps", " ", "\[Delta]t", " ", 
    OverscriptBox["\[Phi]0", "\[Bullet]"]}], "+", 
   RowBox[{
    FractionBox["1", "2"], " ", 
    SuperscriptBox["Eps", "2"], " ", 
    SuperscriptBox["\[Delta]t", "2"], " ", 
    OverscriptBox["\[Phi]0", "\[Bullet]\[Bullet]"]}]}], "\[Equal]", 
  RowBox[{
   RowBox[{"Eps", " ", "\[Delta]\[Phi]"}], "+", "\[Phi]0"}]}]], "Output",
 CellChangeTimes->{
  3.598959068064973*^9, 3.598961081476933*^9, 3.598961501344668*^9, {
   3.598963109623983*^9, 3.598963137150714*^9}, 3.6082078939626493`*^9, 
   3.618656320175604*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[Delta]tSol", "=", 
