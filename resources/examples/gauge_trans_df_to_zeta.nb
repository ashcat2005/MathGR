   3.59895825745719*^9, 3.5989590747484407`*^9, 3.598961082275288*^9, 
   3.598961501371194*^9, {3.5989631096519117`*^9, 3.5989631371801777`*^9}, 
   3.608207894059708*^9}]
}, Open  ]]
