Cell[CellGroupData[{

Cell["Helper functions", "Subsection",
 CellChangeTimes->{{3.598958742490094*^9, 3.598958744430213*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"expandAtXZG", "[", "f_", "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"f", "+", 
       RowBox[{"Eps", "  ", 
        OverscriptBox["f", "\[Bullet]"], " ", "\[Delta]t"}], "+", 
       RowBox[{
        FractionBox[
         SuperscriptBox["Eps", "2"], "2"], 
        OverscriptBox["f", "\[Bullet]\[Bullet]"], 
        SuperscriptBox["\[Delta]t", "2"]}], "+", 
       RowBox[{
        SuperscriptBox["Eps", "2"], 
        RowBox[{"Pd", "[", 
         RowBox[{"f", ",", 
          RowBox[{"DN", "@", "#"}]}], "]"}], 
        RowBox[{"\[Delta]x", "[", 
         RowBox[{"DN", "@", "#"}], "]"}]}]}], ")"}], "&"}], "@", 
    RowBox[{"Uq", "[", "1", "]"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.598958835708775*^9, 3.5989589308890142`*^9}, {
  3.598959471138781*^9, 3.598959501581951*^9}, {3.598961075369176*^9, 
  3.598961075420121*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"taylorSolve", "[", 
   RowBox[{"eqRaw_", ",", " ", "var_", ",", " ", "maxOrder_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "xSeries", ",", " ", "eq", ",", "eqOrder", ",", " ", "x", ",", 
      "solOrder", ",", 
      RowBox[{"solutions", "=", 
       RowBox[{"{", "}"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"xSeries", "=", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         SuperscriptBox["Eps", 
          RowBox[{"n", "-", "1"}]], 
         RowBox[{"x", "[", "n", "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "1", ",", "maxOrder"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"eq", "=", 
      RowBox[{"Normal", "[", 
       RowBox[{"eqRaw", "/.", 
        RowBox[{"var", "\[Rule]", "xSeries"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", " ", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"eqOrder", "=", 
         RowBox[{
          RowBox[{
           RowBox[{"Series", "[", 
            RowBox[{"eq", ",", 
             RowBox[{"{", 
              RowBox[{"Eps", ",", "0", ",", " ", "order"}], "}"}]}], "]"}], "/.",
            "solutions"}], "//", "Normal"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"solOrder", "=", 
         RowBox[{"Flatten", "@", 
          RowBox[{"Solve", "[", 
           RowBox[{"eqOrder", ",", 
            RowBox[{"x", "[", "order", "]"}]}], "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"solutions", "=", 
         RowBox[{"solutions", "~", "Join", "~", "solOrder"}]}], ";"}], 
       "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"order", ",", " ", "0", ",", " ", "maxOrder"}], "}"}]}], 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"var", "\[Rule]", "xSeries"}], "/.", "solutions"}], "}"}]}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.5989520793555603`*^9, 3.598952173554371*^9}, {
   3.598952249602633*^9, 3.598952302462269*^9}, 3.598952333851481*^9, {
   3.5989523814946203`*^9, 3.598952456292314*^9}, {3.598952488522211*^9, 
   3.598952578568738*^9}, {3.598952609145423*^9, 3.5989526244712687`*^9}, {
   3.598952953244719*^9, 3.5989529542468233`*^9}, {3.598953542098502*^9, 
   3.598953570837439*^9}, {3.59895405420584*^9, 3.5989540567414713`*^9}, {
   3.598954154948924*^9, 3.5989541925984907`*^9}, {3.598954315263928*^9, 
   3.5989543748481503`*^9}, {3.598954406489955*^9, 3.598954418543297*^9}, {
   3.598954507134273*^9, 3.598954509464459*^9}, {3.598954558177927*^9, 
   3.598954592307317*^9}, {3.598954667851802*^9, 3.5989547336866293`*^9}, {
   3.598955117946437*^9, 3.598955121584271*^9}, {3.598956371106509*^9, 
   3.598956385238947*^9}, {3.5989565700604973`*^9, 3.598956570810535*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"colSimp", "=", 
   RowBox[{
    RowBox[{"Collect", "[", 
     RowBox[{"#", ",", 
      RowBox[{"{", "Eps", "}"}], ",", "Simp"}], "]"}], "&"}]}], 
  ";"}]], "Input",
