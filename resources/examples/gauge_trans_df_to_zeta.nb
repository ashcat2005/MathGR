Cell[CellGroupData[{

Cell["\<\
We only treat 3 spatial dimensions as abstract\
\>", "Subsection",
 CellChangeTimes->{{3.598960696448228*^9, 3.598960719901226*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"DefaultDim", "=", "3"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SimpHook", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"Pd", "[", 
       RowBox[{"a", ",", 
        RowBox[{"DE", "@", "0"}]}], "]"}], "\[Rule]", 
      RowBox[{"a", "*", "H"}]}], ",", 
     RowBox[{
      RowBox[{"Pd", "[", 
       RowBox[{"H", ",", 
        RowBox[{"DE", "@", "0"}]}], "]"}], "\[Rule]", 
      RowBox[{
       RowBox[{"-", "\[Epsilon]"}], "*", "H", "*", "H"}]}], ",", 
     RowBox[{
      RowBox[{"Pd", "[", 
       RowBox[{"\[Epsilon]", ",", 
        RowBox[{"DE", "@", "0"}]}], "]"}], "\[Rule]", 
      RowBox[{"H", "*", "\[Epsilon]", "*", "\[Eta]"}]}], ",", 
     RowBox[{
      RowBox[{"Pd", "[", 
       RowBox[{"\[Eta]", ",", 
        RowBox[{"DE", "@", "0"}]}], "]"}], "\[Rule]", 
      RowBox[{"H", "*", "\[Eta]2", "*", "\[Eta]"}]}], ",", 
     RowBox[{
      RowBox[{"PdT", "[", 
       RowBox[{"H", ",", 
        RowBox[{"PdVars", "[", 
         RowBox[{
          RowBox[{"DE", "@", "0"}], ",", 
          RowBox[{"DE", "@", "0"}]}], "]"}]}], "]"}], "\[Rule]", 
      RowBox[{
       RowBox[{"2", " ", 
        RowBox[{"H", "^", "3"}], " ", 
        RowBox[{"\[Epsilon]", "^", "2"}]}], "-", 
       RowBox[{
        RowBox[{"H", "^", "3"}], " ", "\[Epsilon]", " ", "\[Eta]"}]}]}], ",", 
     
     RowBox[{
      RowBox[{"PdT", "[", 
       RowBox[{"a", ",", 
        RowBox[{"PdVars", "[", 
         RowBox[{
          RowBox[{"DE", "@", "0"}], ",", 
          RowBox[{"DE", "@", "0"}]}], "]"}]}], "]"}], "\[Rule]", 
      RowBox[{
       RowBox[{"a", " ", 
        RowBox[{"H", "^", "2"}]}], "-", 
       RowBox[{"a", " ", 
        RowBox[{"H", "^", "2"}], " ", "\[Epsilon]"}]}]}]}], "}"}]}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.598960722240116*^9, 3.5989607275674353`*^9}, {
  3.598963071390411*^9, 3.59896309852225*^9}}]
}, Open  ]],

