Cell[CellGroupData[{

Cell["\<\
Gauge transformation of the metric\
\>", "Section",
 CellChangeTimes->{{3.598957030789186*^9, 3.598957035033537*^9}}],

Cell[TextData[{
 "The metric transforms as \n\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["g", "FG\[Alpha]\[Beta]"], "(", 
     SubscriptBox["x", "FG"], ")"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox[
         SubscriptBox["x", "ZG"], "\[Mu]"]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox[
         SubscriptBox["x", "FG"], "\[Alpha]"]}]], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox[
         SubscriptBox["x", "ZG"], "\[Nu]"]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox[
         SubscriptBox["x", "FG"], "\[Beta]"]}]], 
      RowBox[{
       SubscriptBox["g", "ZG\[Mu]\[Nu]"], "(", 
       SubscriptBox["x", "ZG"], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["x", "FG"], "+", "\[Delta]x"}], ")"}], "\[Mu]"]}], 
        RowBox[{"\[PartialD]", 
         SuperscriptBox[
          SubscriptBox["x", "FG"], "\[Alpha]"]}]], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["x", "FG"], "+", "\[Delta]x"}], ")"}], "\[Nu]"]}], 
        RowBox[{"\[PartialD]", 
         SuperscriptBox[
          SubscriptBox["x", "FG"], "\[Beta]"]}]], 
       RowBox[{
        SubscriptBox["g", "ZG\[Mu]\[Nu]"], "(", 
        RowBox[{
         SubscriptBox["x", "FG"], "+", "\[Delta]x"}], ")"}]}], "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox[
          SuperscriptBox["\[Delta]", "\[Mu]"], "\[Alpha]"], "+", 
         FractionBox[
          RowBox[{"\[PartialD]", 
           SuperscriptBox["\[Delta]x", "\[Mu]"]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox[
            SubscriptBox["x", "FG"], "\[Alpha]"]}]]}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox[
          SuperscriptBox["\[Delta]", "\[Nu]"], "\[Beta]"], "+", 
         FractionBox[
          RowBox[{"\[PartialD]", 
           SuperscriptBox["\[Delta]x", "\[Nu]"]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox[
            SubscriptBox["x", "FG"], "\[Beta]"]}]]}], ")"}], 
       RowBox[{
        SubscriptBox["g", "ZG\[Mu]\[Nu]"], "(", 
        RowBox[{
         SubscriptBox["x", "FG"], "+", "\[Delta]x"}], ")"}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      SubscriptBox["g", "ZG\[Alpha]\[Beta]"], "(", 
      RowBox[{
       SubscriptBox["x", "FG"], "+", "\[Delta]x"}], ")"}], "+", 
     RowBox[{"2", 
      RowBox[{
       SubscriptBox["g", "ZG\[Mu]\[Beta]"], "(", 
       RowBox[{
        SubscriptBox["x", "FG"], "+", "\[Delta]x"}], ")"}], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["\[Delta]x", "\[Mu]"]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox[
         SubscriptBox["x", "FG"], "\[Alpha]"]}]]}], "+", 
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["\[Delta]x", "\[Mu]"]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox[
         SubscriptBox["x", "FG"], "\[Alpha]"]}]], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["\[Delta]x", "\[Nu]"]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox[
         SubscriptBox["x", "FG"], "\[Beta]"]}]], 
      RowBox[{
       SubscriptBox["g", "ZG\[Mu]\[Nu]"], "(", 
       RowBox[{
        SubscriptBox["x", "FG"], "+", "\[Delta]x"}], ")"}]}]}]}], 
   TraditionalForm]}],
  FormatType->"TraditionalForm"],
 "\n\nThe 00, 0i, ij components of the above equation is to be solved \
respectively."
}], "Text",
 CellChangeTimes->{{3.5989570424152193`*^9, 3.5989570488600693`*^9}, {
  3.5989572964135237`*^9, 3.5989574805983353`*^9}, {3.598957530228635*^9, 
  3.598957699484982*^9}, {3.598957949963277*^9, 3.598958035296012*^9}}],

Cell[CellGroupData[{

Cell["The ij-component", "Subsection",
 CellChangeTimes->{{3.5989581802855186`*^9, 3.598958190312768*^9}}],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["a", "2"], 
     SubscriptBox["\[Delta]", "ij"]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["g", "FGij"], "(", 
      SubscriptBox["x", "FG"], ")"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["g", "ZGij"], "(", 
        RowBox[{
         SubscriptBox["x", "FG"], "+", "\[Delta]x"}], ")"}], "+", 
       RowBox[{"2", 
        RowBox[{
         SubscriptBox["g", "ZG\[Mu]j"], "(", 
         RowBox[{
          SubscriptBox["x", "FG"], "+", "\[Delta]x"}], ")"}], 
        FractionBox[
         RowBox[{"\[PartialD]", 
          SuperscriptBox["\[Delta]x", "\[Mu]"]}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox[
           SubscriptBox["x", "FG"], "i"]}]]}], "+", 
       RowBox[{
        FractionBox[
         RowBox[{"\[PartialD]", 
          SuperscriptBox["\[Delta]x", "\[Mu]"]}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox[
           SubscriptBox["x", "FG"], "i"]}]], 
        FractionBox[
         RowBox[{"\[PartialD]", 
          SuperscriptBox["\[Delta]x", "\[Nu]"]}], 
